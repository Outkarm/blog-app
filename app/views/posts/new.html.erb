<div>
  <%= form_with model: @posts, url: user_posts_path, data: { turbo: false } do |form| %>
    <div>
      <%= form.label :title %>
      <%= form.text_field :title%>
    </div>
    <div class="mb-4">
      <%= form.label :text %>
      <%= form.text_area :text %>
    </div>
    <div>
      <%= form.submit "Create Post" %>
    </div>
  <% end %>
</div>